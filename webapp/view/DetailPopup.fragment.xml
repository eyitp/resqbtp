<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
    <Dialog id="PopupDialog" width="50em">
        <customHeader>
            <Toolbar>
                <Title text="Cloud Readiness" />
                <ToolbarSpacer />
                <Button icon="sap-icon://sys-cancel" text="Close" press="OnDetailPopupClose" type="Emphasized" />
            </Toolbar>
        </customHeader>
        <Table id="idDetailPopUpTable" inset="true" class="sapUiTinyMargin" items="{CloudReadinessData>/}">
            <columns>
                <Column >
                    <Text text="Object Type" />
                </Column>
                <Column>
                    <Text text="Object Name" />
                </Column>
                <Column>
                    <Text text="Sub Object Name" />
                </Column>
                <Column >
                    <Text text="Error Description" />
                </Column>
                <Column>
                    <Text text="Description" />
                </Column>
                <Column>
                    <Text text="Operation Code" />
                </Column>
            </columns>
            <items>
                <ColumnListItem vAlign="Middle">
                    <cells>
                        <Text text="{CloudReadinessData>ObjectType}" />
                        <Text text="{CloudReadinessData>ObjectName}" />
                        <Text text="{CloudReadinessData>SubObjectName}" />
                        <Text text="{CloudReadinessData>ErrorDescription}" />
                        <Text text="{CloudReadinessData>ComDescriptionpany}" />
                        <Text text="{CloudReadinessData>OperationCode}" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Dialog>
</core:FragmentDefinition>

<core:FragmentDefinition xmlns="sap.m" xmlns:card="sap.f.cards" xmlns:ui="sap.ui.table" xmlns:u="sap.ui.unified" xmlns:core="sap.ui.core" xmlns:f="sap.f" xmlns:mchart="sap.suite.ui.microchart" xmlns:w="sap.ui.integration.widgets">
    <Page id="Build" showHeader="false" backgroundDesign="Transparent">
        <VBox backgroundDesign="Transparent">
            <f:Card class="sapUiTinyMargin" width="auto">
                <f:content>
                    <Title class="sapUiTinyMargin" text="LCNC Lobby" />
                </f:content>
            </f:Card>
            <HBox>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Transform to BTP Cloud" subheader="(BrownField Approach)" frameType="TwoByOne" press="OnBuildToCloudPress">
                    <TileContent footer="On Premise to BTP Cloud (Clean Core)">
                        <HBox renderType="Bare">
                            <FeedContent contentText="Launch LCNC Transform Services" />
                            <ImageContent src="sap-icon://synchronize" />
                        </HBox>
                    </TileContent>
                </GenericTile>
                <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Build in BTP Cloud" subheader="(GreenField Approach)" frameType="TwoByOne" press="OnBuildInBTPCloud">
                    <TileContent footer="Create New in BTP environment (Whitelisted CDS/APIs)">
                        <HBox renderType="Bare">
                            <FeedContent contentText="Launch LCNC Build  Services" />
                            <ImageContent src="sap-icon://upload-to-cloud" />
                        </HBox>
                    </TileContent>
                </GenericTile>
            </HBox>
        </VBox>
    </Page>
    <Page id="TransToCloudBTP" showHeader="false" backgroundDesign="Transparent">
        <VBox>
            <f:Card class="sapUiTinyMargin" width="auto">
                <f:content>
                    <VBox>
                        <HBox>
                            <Button class="sapUiTinyMargin" icon="sap-icon://nav-back" press="OnBuildBack" />
                            <Title class="sapUiTinyMargin sapUiSmallMarginTop" text="Transform to Cloud BTP" />
                        </HBox>
                    </VBox>
                </f:content>
            </f:Card>
            <IconTabBar id="idIconTabBar" select=".onFilterSelect" class="sapUiNoMargin">
                <items>
                    <IconTabFilter text="Programs" key="Programs">
                        <Wizard id="CreateProductWizard" showNextButton="false" renderMode="Page" complete="wizardCompletedHandler">
                            <WizardStep id="AnalyseCATResult" title="Analyse CAT Result" validated="true" />
                            <WizardStep id="ALVAdaptor" title="Create Wrapper API" validated="true">
                                <HBox alignItems="Center">
                                    <VBox>
                                        <Label text="Program Name:" />
                                        <Input />
                                    </VBox>
                                    <VBox class="sapUiSmallMarginBegin">
                                        <Label text="Program Type" />
                                        <Input />
                                    </VBox>
                                    <VBox class="sapUiSmallMarginBegin">
                                        <Label text="Code Complexity" />
                                        <Input />
                                    </VBox>
                                </HBox>
                                <Table id="idProgramsMigrat" mode="SingleSelectLeft" inset="true" items="{Programs>/}">
                                    <columns>
                                        <Column width="12em">
                                            <Text text="Program" />
                                        </Column>
                                        <Column>
                                            <Text text="DB Tables" />
                                        </Column>
                                        <Column width="4em">
                                            <Text text="FMs" />
                                        </Column>
                                        <Column>
                                            <Text text="DB Modify" />
                                        </Column>
                                        <Column>
                                            <Text text="Call TCode" />
                                        </Column>
                                        <Column>
                                            <Text text="Code Complexity" />
                                        </Column>
                                        <Column>
                                            <Text text="Status" />
                                        </Column>
                                    </columns>
                                    <items>
                                        <ColumnListItem vAlign="Middle">
                                            <cells>
                                                <Text text="{Programs>Program}" />
                                                <ObjectNumber inverted="true" number="{Programs>CountDBTable}" />
                                                <ObjectNumber inverted="true" number="{Programs>CountFM}" />
                                                <Text text="{Programs>DBModify}" />
                                                <Text text="{Programs>CLLTCode}" />
                                                <Text text="{Programs>CodeComp}" />
                                                <ObjectStatus
                                                    text="{=${Programs>Status}==='NI'?'Not Initiated':'' || ${Programs>Status}==='IP'?'RAP Build In Progress':'BTP Deployment Successful'}"
                                                    icon="{=${Programs>Status}==='NI'?'sap-icon://stop':'' || ${Programs>Status}==='IP'?'sap-icon://status-inactive':'sap-icon://message-success'}"
                                                    state="{=${Programs>Status}==='NI'?'None':'' || ${Programs>Status}==='IP'?'Warning':'Success'}"
                                                />
                                            </cells>
                                        </ColumnListItem>
                                    </items>
                                </Table>
                                <HBox justifyContent="End">
                                    <Button text="Lift and Shift" press="MigrateToBTPPress" type="Emphasized" />
                                </HBox>
                                <!-- </VBox> -->
                            </WizardStep>
                            <WizardStep id="RAPBuilder" title="RAP Builder" validated="true">
                                <VBox>
                                    <HBox alignItems="Center" visible="{AppConfig>/MoveToFioriBOTBuilder}">
                                        <HBox width="50%">
                                            <MessageStrip class="sapUiTinyMargin" type="Success" text="RAP Build successfully." showIcon="true" />
                                        </HBox>
                                        <HBox width="50%" justifyContent="End">
                                            <Button text="Next Step" type="Emphasized" press="OnNextStepForFioriBuild" />
                                        </HBox>
                                    </HBox>

                                    <ObjectHeader icon="sap-icon://sap-box" iconDensityAware="false" iconAlt="Denise Smith" imageShape="Circle" responsive="true" title="{SelectedRow>/Program}" intro="{SelectedRow>/Type}" class="sapUiResponsivePadding--header">
                                        <ObjectAttribute title="DB Tables" text="{SelectedRow>/CountDBTable}" />
                                        <ObjectAttribute title="Function Modules" text="{SelectedRow>/CountFM}" />
                                        <ObjectAttribute title="DB Modify" text="{SelectedRow>/DBModify}" />
                                        <ObjectAttribute title="Call TCode" text="{SelectedRow>/CLLTCode}" />
                                        <ObjectAttribute title="Code Complexity" text="{SelectedRow>/CodeComp}" />
                                        <statuses>
                                            <ObjectStatus
                                                class="sapUiSmallMarginBottom"
                                                title="Status"
                                                text="{=${SelectedRow>/Status}==='NI'?'Not Initiated':'' || ${SelectedRow>/Status}==='IP'?'RAP Build In Progress':'RAP Build Successful'}"
                                                inverted="true"
                                                state="{=${SelectedRow>/Status}==='NI'?'None':'' || ${SelectedRow>/Status}==='IP'?'Warning':'Success'}"
                                            />
                                        </statuses>
                                    </ObjectHeader>
                                    <HBox justifyContent="Center">
                                        <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto" height="12em">
                                            <f:header>
                                                <card:Header title="Custom Entity Creation" subtitle="complete(%)" />
                                            </f:header>
                                            <f:content>
                                                <mchart:RadialMicroChart id="CustomEntityChart" class="sapUiTinyMargin sapUiSmallMarginTop" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                            </f:content>
                                        </f:Card>
                                        <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto">
                                            <f:header>
                                                <card:Header title="Custom Query Class Creation" subtitle="complete(%)" />
                                            </f:header>
                                            <f:content>
                                                <mchart:RadialMicroChart busy="true" id="CustomQueryChart" class="sapUiTinyMargin" size="L" percentage="100" alignContent="Center" valueColor="Good" />
                                            </f:content>
                                        </f:Card>
                                        <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto">
                                            <f:header>
                                                <card:Header title="Service Definition Creation" subtitle="complete(%)" />
                                            </f:header>
                                            <f:content>
                                                <mchart:RadialMicroChart busy="true" id="ServiceDefinitionChart" class="sapUiTinyMargin" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                            </f:content>
                                        </f:Card>
                                        <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto" height="12em">
                                            <f:header>
                                                <card:Header title="Service binding" subtitle="complete(%)" />
                                            </f:header>
                                            <f:content>
                                                <mchart:RadialMicroChart busy="true" id="ServiceBindingChart" class="sapUiTinyMargin sapUiSmallMarginTop" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                            </f:content>
                                        </f:Card>
                                    </HBox>
                                </VBox>
                            </WizardStep>
                            <WizardStep id="FioriRAPBuilder" title="Fiori RAP Builder" validated="true">
                                <MessageStrip visible="{=${SelectedRow>/Status}==='C'}" class="sapUiTinyMargin" type="Success" text="Report ({SelectedRow>/Program}) Migrated to BTP and FIORI app deployed in BTP Launchpad successfully. Click to Launch app from " showIcon="true">
                                    <link>
                                        <Link text="BTP FIORI Launchpad" target="_blank" href="https://e42f23c5trial.launchpad.cfapps.us10.hana.ondemand.com/site?siteId=e1b23256-742f-4f91-b896-b2d4a034ad34#Shell-home" />
                                    </link>
                                </MessageStrip>
                                <ObjectHeader icon="sap-icon://sap-box" iconDensityAware="false" iconAlt="Denise Smith" imageShape="Circle" responsive="true" title="{SelectedRow>/Program}" intro="{SelectedRow>/Type}" class="sapUiResponsivePadding--header">
                                    <ObjectAttribute title="DB Tables" text="{SelectedRow>/CountDBTable}" />
                                    <ObjectAttribute title="Function Modules" text="{SelectedRow>/CountFM}" />
                                    <ObjectAttribute title="DB Modify" text="{SelectedRow>/DBModify}" />
                                    <ObjectAttribute title="Call TCode" text="{SelectedRow>/CLLTCode}" />
                                    <ObjectAttribute title="Code Complexity" text="{SelectedRow>/CodeComp}" />
                                    <statuses>
                                        <ObjectStatus
                                            class="sapUiSmallMarginBottom"
                                            title="Status"
                                            text="{=${SelectedRow>/Status}==='NI'?'Not Initiated':'' || ${SelectedRow>/Status}==='IP'?'Fiori RAP Build In Progress':'BTP Deployment Successful'}"
                                            inverted="true"
                                            state="{=${SelectedRow>/Status}==='NI'?'None':'' || ${SelectedRow>/Status}==='IP'?'Warning':'Success'}"
                                        />
                                    </statuses>
                                </ObjectHeader>

                                <HBox justifyContent="Center">
                                    <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto" height="12em">
                                        <f:header>
                                            <card:Header title="Fiori List Report Creation" subtitle="complete(%)" />
                                        </f:header>
                                        <f:content>
                                            <mchart:RadialMicroChart id="idFioriListReportCreationChart" class="sapUiTinyMargin" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                        </f:content>
                                    </f:Card>
                                    <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto" height="12em">
                                        <f:header>
                                            <card:Header title="Fiori Tile Configuration" subtitle="complete(%)" />
                                        </f:header>
                                        <f:content>
                                            <mchart:RadialMicroChart busy="true" id="idFioriTileConfigurationChart" class="sapUiTinyMargin" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                        </f:content>
                                    </f:Card>
                                </HBox>
                            </WizardStep>
                        </Wizard>
                    </IconTabFilter>
                    <IconTabFilter text="Classes" key="Classes">
                        <Wizard id="ClassesWizard" showNextButton="false" renderMode="Page" complete="wizardCompletedHandler">
                            <WizardStep id="AnalyseClassCATResult" title="Analyse CAT Result" validated="true" />
                            <WizardStep id="ClassALVAdaptor" title="Remediate" validated="true">
                                <HBox alignItems="Center">
                                    <VBox>
                                        <Label text="Program Name:" />
                                        <Input />
                                    </VBox>
                                    <VBox class="sapUiSmallMarginBegin">
                                        <Label text="Program Type" />
                                        <Input />
                                    </VBox>
                                    <VBox class="sapUiSmallMarginBegin">
                                        <Label text="Code Complexity" />
                                        <Input />
                                    </VBox>
                                </HBox>
                                <Table id="idClassMigrat" mode="SingleSelectLeft" inset="true" items="{Programs>/}">
                                    <columns>
                                        <Column width="12em">
                                            <Text text="Program" />
                                        </Column>
                                        <Column>
                                            <Text text="DB Tables" />
                                        </Column>
                                        <Column width="4em">
                                            <Text text="FMs" />
                                        </Column>
                                        <Column>
                                            <Text text="DB Modify" />
                                        </Column>
                                        <Column>
                                            <Text text="Call TCode" />
                                        </Column>
                                        <Column>
                                            <Text text="Code Complexity" />
                                        </Column>
                                        <Column>
                                            <Text text="Status" />
                                        </Column>
                                    </columns>
                                    <items>
                                        <ColumnListItem vAlign="Middle">
                                            <cells>
                                                <Text text="{Programs>Program}" />
                                                <ObjectNumber inverted="true" number="{Programs>CountDBTable}" />
                                                <ObjectNumber inverted="true" number="{Programs>CountFM}" />
                                                <Text text="{Programs>DBModify}" />
                                                <Text text="{Programs>CLLTCode}" />
                                                <Text text="{Programs>CodeComp}" />
                                                <ObjectStatus
                                                    text="{=${Programs>Status}==='NI'?'Not Initiated':'' || ${Programs>Status}==='IP'?'RAP Build In Progress':'BTP Deployment Successful'}"
                                                    icon="{=${Programs>Status}==='NI'?'sap-icon://stop':'' || ${Programs>Status}==='IP'?'sap-icon://status-inactive':'sap-icon://message-success'}"
                                                    state="{=${Programs>Status}==='NI'?'None':'' || ${Programs>Status}==='IP'?'Warning':'Success'}"
                                                />
                                            </cells>
                                        </ColumnListItem>
                                    </items>
                                </Table>
                                <HBox justifyContent="End">
                                    <Button class="sapUiTinyMargin" text="Remediate" type="Emphasized" />
                                    <Button class="sapUiTinyMargin" text="Lift and Shift" press="MigrateToBTPPress" type="Emphasized" />
                                    <Button class="sapUiTinyMargin" text="Wrap" type="Emphasized" />
                                </HBox>
                                <!-- </VBox> -->
                            </WizardStep>
                            <WizardStep id="ClassRAPBuilder" title="Export to GitHub" validated="true">
                                <VBox>
                                    <HBox alignItems="Center" visible="{AppConfig>/MoveToFioriBOTBuilder}">
                                        <HBox width="50%">
                                            <MessageStrip class="sapUiTinyMargin" type="Success" text="RAP Build successfully." showIcon="true" />
                                        </HBox>
                                        <HBox width="50%" justifyContent="End">
                                            <Button text="Next Step" type="Emphasized" press="OnNextStepForFioriBuild" />
                                        </HBox>
                                    </HBox>

                                    <ObjectHeader icon="sap-icon://sap-box" iconDensityAware="false" iconAlt="Denise Smith" imageShape="Circle" responsive="true" title="{SelectedRow>/Program}" intro="{SelectedRow>/Type}" class="sapUiResponsivePadding--header">
                                        <ObjectAttribute title="DB Tables" text="{SelectedRow>/CountDBTable}" />
                                        <ObjectAttribute title="Function Modules" text="{SelectedRow>/CountFM}" />
                                        <ObjectAttribute title="DB Modify" text="{SelectedRow>/DBModify}" />
                                        <ObjectAttribute title="Call TCode" text="{SelectedRow>/CLLTCode}" />
                                        <ObjectAttribute title="Code Complexity" text="{SelectedRow>/CodeComp}" />
                                        <statuses>
                                            <ObjectStatus
                                                class="sapUiSmallMarginBottom"
                                                title="Status"
                                                text="{=${SelectedRow>/Status}==='NI'?'Not Initiated':'' || ${SelectedRow>/Status}==='IP'?'RAP Build In Progress':'RAP Build Successful'}"
                                                inverted="true"
                                                state="{=${SelectedRow>/Status}==='NI'?'None':'' || ${SelectedRow>/Status}==='IP'?'Warning':'Success'}"
                                            />
                                        </statuses>
                                    </ObjectHeader>
                                    <HBox justifyContent="Center">
                                        <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto" height="12em">
                                            <f:header>
                                                <card:Header title="Custom Entity Creation" subtitle="complete(%)" />
                                            </f:header>
                                            <f:content>
                                                <mchart:RadialMicroChart id="CustomEntityClassChart" class="sapUiTinyMargin sapUiSmallMarginTop" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                            </f:content>
                                        </f:Card>
                                        <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto">
                                            <f:header>
                                                <card:Header title="Custom Query Class Creation" subtitle="complete(%)" />
                                            </f:header>
                                            <f:content>
                                                <mchart:RadialMicroChart busy="true" id="CustomQueryClassChart" class="sapUiTinyMargin" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                            </f:content>
                                        </f:Card>
                                        <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto">
                                            <f:header>
                                                <card:Header title="Service Definition Creation" subtitle="complete(%)" />
                                            </f:header>
                                            <f:content>
                                                <mchart:RadialMicroChart busy="true" id="ServiceDefinitionClassChart" class="sapUiTinyMargin" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                            </f:content>
                                        </f:Card>
                                        <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto" height="12em">
                                            <f:header>
                                                <card:Header title="Service binding" subtitle="complete(%)" />
                                            </f:header>
                                            <f:content>
                                                <mchart:RadialMicroChart busy="true" id="ServiceBindingClassChart" class="sapUiTinyMargin sapUiSmallMarginTop" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                            </f:content>
                                        </f:Card>
                                    </HBox>
                                </VBox>
                            </WizardStep>
                            <WizardStep id="FioriClassRAPBuilder" title="Import to BTP" validated="true">
                                <MessageStrip visible="{=${SelectedRow>/Status}==='C'}" class="sapUiTinyMargin" type="Success" text="Report ({SelectedRow>/Program}) Migrated to BTP and FIORI app deployed in BTP Launchpad successfully. Click to Launch app from " showIcon="true">
                                    <link>
                                        <Link text="BTP FIORI Launchpad" target="_blank" href="https://e42f23c5trial.launchpad.cfapps.us10.hana.ondemand.com/site?siteId=e1b23256-742f-4f91-b896-b2d4a034ad34#Shell-home" />
                                    </link>
                                </MessageStrip>
                                <ObjectHeader icon="sap-icon://sap-box" iconDensityAware="false" iconAlt="Denise Smith" imageShape="Circle" responsive="true" title="{SelectedRow>/Program}" intro="{SelectedRow>/Type}" class="sapUiResponsivePadding--header">
                                    <ObjectAttribute title="DB Tables" text="{SelectedRow>/CountDBTable}" />
                                    <ObjectAttribute title="Function Modules" text="{SelectedRow>/CountFM}" />
                                    <ObjectAttribute title="DB Modify" text="{SelectedRow>/DBModify}" />
                                    <ObjectAttribute title="Call TCode" text="{SelectedRow>/CLLTCode}" />
                                    <ObjectAttribute title="Code Complexity" text="{SelectedRow>/CodeComp}" />
                                    <statuses>
                                        <ObjectStatus
                                            class="sapUiSmallMarginBottom"
                                            title="Status"
                                            text="{=${SelectedRow>/Status}==='NI'?'Not Initiated':'' || ${SelectedRow>/Status}==='IP'?'Fiori RAP Build In Progress':'BTP Deployment Successful'}"
                                            inverted="true"
                                            state="{=${SelectedRow>/Status}==='NI'?'None':'' || ${SelectedRow>/Status}==='IP'?'Warning':'Success'}"
                                        />
                                    </statuses>
                                </ObjectHeader>

                                <HBox justifyContent="Center">
                                    <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto" height="12em">
                                        <f:header>
                                            <card:Header title="Fiori List Report Creation" subtitle="complete(%)" />
                                        </f:header>
                                        <f:content>
                                            <mchart:RadialMicroChart id="idFioriListReportClassCreationChart" class="sapUiTinyMargin" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                        </f:content>
                                    </f:Card>
                                    <f:Card class="sapUiTinyMargin CustomTileBGColor" width="auto" height="12em">
                                        <f:header>
                                            <card:Header title="Fiori Tile Configuration" subtitle="complete(%)" />
                                        </f:header>
                                        <f:content>
                                            <mchart:RadialMicroChart busy="true" id="idFioriTileClassConfigurationChart" class="sapUiTinyMargin" size="L" percentage="0" alignContent="Center" valueColor="Good" />
                                        </f:content>
                                    </f:Card>
                                </HBox>
                            </WizardStep>
                        </Wizard>
                    </IconTabFilter>
                    <IconTabFilter text="FMs" key="FMs">
                        <Wizard id="FMsWizard" showNextButton="false" renderMode="Page" complete="wizardCompletedHandler">
                            <WizardStep id="AnalyseFMsCATResult" title="Analyse CAT Result" validated="true" />
                            <WizardStep id="FMsALVAdaptor" title="Remediate" validated="true">
                                <HBox alignItems="Center">
                                    <VBox>
                                        <Label text="FM Name:" />
                                        <Input />
                                    </VBox>
                                    <VBox class="sapUiSmallMarginBegin">
                                        <Label text="RFC Enabled" />
                                        <Input />
                                    </VBox>
                                    <VBox class="sapUiSmallMarginBegin">
                                        <Label text="Remediation Type" />
                                        <Input />
                                    </VBox>
                                </HBox>
                                <Table id="idFMsMigrat" mode="SingleSelectLeft" inset="true" items="{FMs>/}">
                                    <columns>
                                        <Column width="12em">
                                            <Text text="FM Name" />
                                        </Column>
                                        <Column>
                                            <Text text="RFC" />
                                        </Column>
                                        <Column width="12em">
                                            <Text text="Code Complexity" />
                                        </Column>
                                        <Column width="12em">
                                            <Text text="Remediation Type" />
                                        </Column>
                                        <Column>
                                            <Text text="Status" />
                                        </Column>
                                        <Column>
                                            <Text text="Retire" />
                                        </Column>
                                    </columns>
                                    <items>
                                        <ColumnListItem vAlign="Middle">
                                            <cells>
                                                <Text text="{FMs>Program}" />
                                                <Text text="{FMs>RFC}" />
                                                <Text text="{FMs>CodeComp}" />
                                                <ObjectStatus text="{FMs>RemediationType}" />
                                                <Text text="{FMs>Status}" />
                                                <CheckBox />
                                            </cells>
                                        </ColumnListItem>
                                    </items>
                                </Table>
                                <HBox justifyContent="End">
                                    <Button class="sapUiTinyMargin" text="Remediate" type="Emphasized" press="onFMRemediatePress" />
                                    <Button class="sapUiTinyMargin" text="Lift and Shift" press="onFMsLiftAndShiftPress" type="Emphasized" />
                                    <Button class="sapUiTinyMargin" text="Wrap" type="Emphasized" press="onFMsWrapPress" />
                                </HBox>
                                <!-- </VBox> -->
                            </WizardStep>
                            <WizardStep id="FMsRAPBuilder" title="Export to GitHub" validated="true">
                                <VBox>
                                    <HBox alignItems="Center" visible="{AppConfig>/FMImportToBTP}">
                                        <HBox width="50%">
                                            <MessageStrip class="sapUiTinyMargin" type="Success" text="RAP Build successfully." showIcon="true" />
                                        </HBox>
                                        <HBox width="50%" justifyContent="End">
                                            <Button text="Next Step" type="Emphasized" press="OnNextStepForFMFioriBuild" />
                                        </HBox>
                                    </HBox>
                                    <u:FileUploader id="fileFMUploader" name="myFileFMUpload" uploadUrl="upload/" tooltip="Upload your file to the local server"/>
                                    <Button text="Import to BTP" press="handleFMUpload" class="sapUiSmallMarginBottom"/>
                                    <ObjectHeader icon="sap-icon://sap-box" iconDensityAware="false" iconAlt="Denise Smith" imageShape="Circle" responsive="true" title="{SelectedFMRow>/Program}" intro="{SelectedFMRow>/Type}" class="sapUiResponsivePadding--header">
                                        <ObjectAttribute title="RFC" text="{SelectedFMRow>/RFC}" />
                                        <ObjectAttribute title="Remediation Type" text="{SelectedFMRow>/RemediationType}" />
                                        <ObjectAttribute title="Code Complexity" text="{SelectedFMRow>/CodeComp}" />
                                        <statuses>
                                            <ObjectStatus
                                                class="sapUiSmallMarginBottom"
                                                title="Status"
                                                text="{=${SelectedFMRow>/Status}==='NI'?'Not Initiated':'' || ${SelectedFMRow>/Status}==='Cloud Ready'?'RAP Build In Progress':'RAP Build Successful'}"
                                                inverted="true"
                                                state="{=${SelectedFMRow>/Status}==='NI'?'None':'' || ${SelectedFMRow>/Status}==='Cloud Ready'?'Warning':'Success'}"
                                            />
                                        </statuses>
                                    </ObjectHeader>
                                </VBox>
                            </WizardStep>
                            <WizardStep id="FioriFMsRAPBuilder" title="Import to BTP" validated="true">
                                <MessageStrip visible="{=${SelectedRow>/Status}==='C'}" class="sapUiTinyMargin" type="Success" text="Report ({SelectedRow>/Program}) Migrated to BTP and FIORI app deployed in BTP Launchpad successfully. Click to Launch app from " showIcon="true">
                                    <link>
                                        <Link text="BTP FIORI Launchpad" target="_blank" href="https://e42f23c5trial.launchpad.cfapps.us10.hana.ondemand.com/site?siteId=e1b23256-742f-4f91-b896-b2d4a034ad34#Shell-home" />
                                    </link>
                                </MessageStrip>
                                <ObjectHeader icon="sap-icon://sap-box" iconDensityAware="false" iconAlt="Denise Smith" imageShape="Circle" responsive="true" title="{SelectedFMRow>/Program}" intro="{SelectedFMRow>/Type}" class="sapUiResponsivePadding--header">
                                        <ObjectAttribute title="RFC" text="{SelectedFMRow>/RFC}" />
                                        <ObjectAttribute title="Status" text="{SelectedFMRow>/Status}" />
                                        <ObjectAttribute title="Remediation Type" text="{SelectedFMRow>/RemediationType}" />
                                        <ObjectAttribute title="Code Complexity" text="{SelectedFMRow>/CodeComp}" />
                                        <statuses>
                                            <ObjectStatus
                                                class="sapUiSmallMarginBottom"
                                                title="Status"
                                                text="{=${SelectedFMRow>/Status}==='NI'?'Not Initiated':'' || ${SelectedFMRow>/Status}==='Import to BTP'?'RAP Build In Progress':'Exported to GitHub'}"
                                                inverted="true"
                                                state="{=${SelectedFMRow>/Status}==='NI'?'None':'' || ${SelectedFMRow>/Status}==='Import to BTP'?'Warning':'Success'}"/>
                                        </statuses>
                                    </ObjectHeader>
                            </WizardStep>
                        </Wizard>
                    </IconTabFilter>
                    <IconTabFilter text="Enhancements" key="Enhancments/BADIs" />
                </items>
            </IconTabBar>
        </VBox>
    </Page>
